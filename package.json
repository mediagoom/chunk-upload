{
  "name": "@mediagoom/chunk-upload",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.14",
  "description": "A Browser to node.js chunked uploader",
  "main": "src/server.js",
  "scripts": {
    "build": "rollup -c",
    "test-client": "node ./node_modules/mocha/bin/mocha --timeout 800000 ./test/integration/index.js",
    "test": "node ./node_modules/mocha/bin/mocha ./test/unit//**/*.js",
    "integration": "node ./node_modules/mocha/bin/mocha --timeout 30000 ./test/integration//**/*.spec.js",
    "e2e": "node ./node_modules/mocha/bin/mocha --timeout 30000 ./test/e2e//**/*.js",
    "start": "node ./node_modules/@mediagoom/devman start server",
    "dev": "node ./node_modules/@mediagoom/devman",
    "server": "node ./test/integration/server.js",
    "stop": "node ./node_modules/@mediagoom/devman stop",
    "cov-test": "nyc -x 'src/core/**/*.js' npm run test",
    "cov-server": "nyc npm run server",
    "cov-integration": "nyc --clean false npm run integration",
    "cov-client": "nyc --clean false npm run test-client",
    "cov-html": "nyc report --reporter=html",
    "cov-text": "nyc report --reporter=text",
    "clean": "shx rm -fr ./lib",
    "cleanup": "shx rm -fr ./uploader",
    "clean-cov": "shx rm -fr ./.nyc_output",
    "clean-all": "run-p clean cleanup clean-cov",
    "mv-uploader": "shx mv ./uploader/uploader/coverage.json ./.nyc_output",
    "eslint": "eslint ./src/**/*.js ./test/**/*.js ",
    "test-all": "run-s build test integration start test-client e2e stop"
    , "live" : "live-server --mount=/:./sample --proxy=/upload:http://localhost:3000/upload"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mediagoom/chunk-upload.git"
  },
  "keywords": [
    "upload",
    "chunk"
  ],
  "author": "admin@mediagoom.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mediagoom/chunk-upload/issues"
  },
  "homepage": "https://github.com/mediagoom/chunk-upload#readme",
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/helper-module-imports": "7.0.0",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-transform-object-assign": "7.2.0",
    "@babel/preset-env": "7.3.4",
    "@mediagoom/devman": "0.0.10",
    "autoprefixer": "^9.5.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^5.1.1",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "debug": "^4.1.1",
    "eslint": "^5.15.3",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "live-server": "^1.2.1",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "postcss": "^7.0.14",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-sass": "^1.1.0",
    "sass-inline-svg": "^1.2.0",
    "saucelabs": "^2.1.3",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "shx": "^0.3.2",
    "supertest": "^4.0.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "superagent": "^4.1.0",
    "superagent-proxy": "^2.0.0"
  }
}
