{
  "name": "chunk-upload",
  "version": "0.0.3",
  "description": "A Browser to node.js chunked uploader",
  "main": "bin/index.js",
  "scripts": {
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "test": "node ./node_modules/mocha/bin/mocha --reporter spec --timeout 800000 ./bin//**/test/index.js",
    "start": "node ./node_modules/devman start server",
    "dev": "node ./node_modules/devman",
    "server": "node ./bin/test/server.js",
    "stop": "node ./node_modules/devman stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mediagoom/chunk-upload.git"
  },
  "keywords": [
    "upload",
    "chunk"
  ],
  "author": "admin@mediagoom.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mediagoom/chunk-upload/issues"
  },
  "homepage": "https://github.com/mediagoom/chunk-upload#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "chai": "^3.5.0",
    "chai-files": "^1.4.0",
    "devman": "0.0.3",
    "eslint": "^3.16.1",
    "express": "^4.14.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-util": "^3.0.8",
    "mocha": "^3.2.0",
    "request": "^2.81.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
